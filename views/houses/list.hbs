<!-- Here is the part of the image with the button -->
<div class="container">
	{{>nav}}
	<!-- Here is the part of the navigation bar with ths options and button -->
</div>
<div class="container">
	<nav class="navbar navbar-expand-lg navbar-light bg-light">
		<div class="container-fluid">
			<div class="collapse navbar-collapse">
				<ul class="navbar-nav me-auto mb-2 mb-lg-0">
					<li class="nav-item">
						<div class="col-auto pe-3">
							<div class="input-group">
								<div class="input-group-text">@</div>
								<select class="form-select">
									<option selected>Any location</option>
									<option value="1">One</option>
									<option value="2">Two</option>
								</select>
							</div>
						</div>
					</li>
					<li class="nav-item">
						<div class="col-auto pe-3">
							<div class="input-group">
								<div class="input-group-text">@</div>
								<select class="form-select">
									<option selected>Any rooms</option>
									<option value="1">One</option>
									<option value="2">Two</option>
								</select>
							</div>
						</div>
					</li>
					<li class="nav-item">
						<div class="col-auto pe-3">
							<div class="input-group">
								<div class="input-group-text">$</div>
								<input
									type="number"
									class="form-control"
									min="1"
									placeholder="max price"
								/>
							</div>
						</div>
					</li>
					<li class="nav-item">
						<div class="col-auto pe-3">
							<div class="input-group">
								<div class="input-group-text">#</div>
								<select class="form-select">
									<option selected>Price(low to high)</option>
									<option value="1">One</option>
									<option value="2">Two</option>
								</select>
							</div>
						</div>
					</li>
				</ul>
				<form class="d-flex">
					<input
						class="form-control me-2"
						type="search"
						placeholder="Search"
						aria-label="Search"
					/>
					<button class="btn btn-success" type="submit">
						Search
					</button>
				</form>
			</div>
		</div>
	</nav>
</div>
